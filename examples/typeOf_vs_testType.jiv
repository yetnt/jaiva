tsea "testType.jiv"! @ Imports the testType function and the array variable used in that file.
tsea "jaiva/types" <- t_num! @ Only import these t_num function

@ Add a function to the array
maak new <-| t_num!
arr <- flat(arr, new)!

khuluma("testType()$t|$tt_of()")!
khuluma("-------$t$t|$t-----")!

colonize v with arr ->
    maak out <- testType(v)!
    khuluma(out + ((out != idk => out~ however 6) <= 7 => "$t$t|$t" however "$t|$t") + typeOf(v))!
<~
